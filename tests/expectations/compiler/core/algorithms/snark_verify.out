    async snark_verify_batch r0 r1 r2 into r3;
    output r3 as test_snark_verify.aleo/snark_verify_batch.future;

finalize snark_verify_batch:
    input r0 as [[u8; 2048u32]; 2048u32].public;
    input r1 as [[[field; 2048u32]; 2048u32]; 2048u32].public;
    input r2 as [u8; 2048u32].public;
