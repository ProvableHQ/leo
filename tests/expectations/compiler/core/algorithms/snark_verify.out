program test_snark_verify.aleo;

function snark_verify:
    input r0 as [u8; 2048u32].private;
    input r1 as [field; 2048u32].private;
    input r2 as [u8; 2048u32].private;
    async snark_verify r0 r1 r2 into r3;
    output r3 as test_snark_verify.aleo/snark_verify.future;

finalize snark_verify:
    input r0 as [u8; 2048u32].public;
    input r1 as [field; 2048u32].public;
    input r2 as [u8; 2048u32].public;
    snark.verify r0 r1 r2 into r3;
    assert.eq r3 true;

