DCE_ENABLED:
program test.aleo;

function public_adder:
    input r0 as u8.public;
    input r1 as u8.public;
    async public_adder r0 r1 into r2;
    output r2 as test.aleo/public_adder.future;

finalize public_adder:
    input r0 as u8.public;
    input r1 as u8.public;
    assert.neq r0 r1;
    add r0 r1 into r2;
    assert.neq r2 0u8;

DCE_DISABLED:
program test.aleo;

function public_adder:
    input r0 as u8.public;
    input r1 as u8.public;
    async public_adder r0 r1 into r2;
    output r2 as test.aleo/public_adder.future;

finalize public_adder:
    input r0 as u8.public;
    input r1 as u8.public;
    assert.neq r0 r1;
    add r0 r1 into r2;
    assert.neq r2 0u8;
