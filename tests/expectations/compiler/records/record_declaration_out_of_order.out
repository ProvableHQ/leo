DCE_ENABLED:
program test.aleo;

record Token:
    owner as address.private;
    amount as u64.private;

function main:
    input r0 as u64.private;
    input r1 as u64.private;
    add r0 r1 into r2;
    cast self.caller r2 into r3 as Token.record;
    output r3 as Token.record;

DCE_DISABLED:
program test.aleo;

record Token:
    owner as address.private;
    amount as u64.private;

function main:
    input r0 as u64.private;
    input r1 as u64.private;
    add r0 r1 into r2;
    cast self.caller r2 into r3 as Token.record;
    output r3 as Token.record;
