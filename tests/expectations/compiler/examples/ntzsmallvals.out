program test.aleo;

function main:
    input r0 as u32.public;
    is.eq r0 0u32 into r1;
    shl.w r0 16u8 into r2;
    is.neq r2 0u32 into r3;
    ternary r3 15u8 31u8 into r4;
    ternary r3 r2 r0 into r5;
    shl.w r5 8u8 into r6;
    is.neq r6 0u32 into r7;
    sub r4 8u8 into r8;
    ternary r7 r8 r4 into r9;
    ternary r7 r6 r5 into r10;
    shl.w r10 4u8 into r11;
    is.neq r11 0u32 into r12;
    sub r9 4u8 into r13;
    ternary r12 r13 r9 into r14;
    ternary r12 r11 r10 into r15;
    shl.w r15 2u8 into r16;
    is.neq r16 0u32 into r17;
    sub r14 2u8 into r18;
    ternary r17 r18 r14 into r19;
    ternary r17 r16 r15 into r20;
    shl.w r20 1u8 into r21;
    is.neq r21 0u32 into r22;
    sub r19 1u8 into r23;
    ternary r22 r23 r19 into r24;
    ternary r1 32u8 r24 into r25;
    output r25 as u8.private;
