DCE_ENABLED:
program test.aleo;

function foo:
    input r0 as [boolean; 8u32].private;
    async foo r0 into r1;
    output r1 as test.aleo/foo.future;

finalize foo:
    input r0 as [boolean; 8u32].public;
    assert.eq true true;

DCE_DISABLED:
program test.aleo;

function foo:
    input r0 as [boolean; 8u32].private;
    async foo r0 into r1;
    output r1 as test.aleo/foo.future;

finalize foo:
    input r0 as [boolean; 8u32].public;
    assert.eq true true;
