program test.aleo;

function simple_slices:
    input r0 as [boolean; 1u32].private;
    cast r0[0u32] into r1 as [boolean; 1u32];
    assert.eq r1 r1;
    assert.eq r1 r1;
    assert.eq r1 r1;
    assert.eq r1 r1;
    assert.eq r1 r1;
