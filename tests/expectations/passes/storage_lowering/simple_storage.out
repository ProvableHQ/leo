program test.aleo {
    mapping flag__: bool => bool;
    mapping scalar_val__: bool => scalar;
    mapping field_val__: bool => field;
    mapping group_val__: bool => group;
    async transition initialize() -> Future<Fn()> {
        return async {
            Mapping::set(::flag__, false, true);
            Mapping::set(::scalar_val__, false, 1);
            Mapping::set(::field_val__, false, 42);
            Mapping::set(::group_val__, false, 0);
        };
    }
    async transition to_none() -> Future<Fn()> {
        return async {
            Mapping::remove(::flag__, false);
            Mapping::remove(::scalar_val__, false);
            Mapping::remove(::field_val__, false);
            Mapping::remove(::group_val__, false);
        };
    }
    async transition check1() -> Future<Fn()> {
        return async {
            assert(Optional::unwrap(Mapping::contains(::flag__, false) ? Mapping::get_or_use(::flag__, false, false) : none) == true);
            assert(Optional::unwrap(Mapping::contains(::scalar_val__, false) ? Mapping::get_or_use(::scalar_val__, false, 0scalar) : none) == 1);
            assert(Optional::unwrap(Mapping::contains(::field_val__, false) ? Mapping::get_or_use(::field_val__, false, 0field) : none) == 42);
            assert(Optional::unwrap(Mapping::contains(::group_val__, false) ? Mapping::get_or_use(::group_val__, false, 0group) : none) == 0);
        };
    }
    async transition check2() -> Future<Fn()> {
        return async {
            assert(Optional::unwrap_or(Mapping::contains(::flag__, false) ? Mapping::get_or_use(::flag__, false, false) : none, false) == true);
            assert(Optional::unwrap_or(Mapping::contains(::scalar_val__, false) ? Mapping::get_or_use(::scalar_val__, false, 0scalar) : none, 0) == 1);
            assert(Optional::unwrap_or(Mapping::contains(::field_val__, false) ? Mapping::get_or_use(::field_val__, false, 0field) : none, 0) == 42);
            assert(Optional::unwrap_or(Mapping::contains(::group_val__, false) ? Mapping::get_or_use(::group_val__, false, 0group) : none, 0) == 0);
        };
    }
}
