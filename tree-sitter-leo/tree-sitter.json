{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "leo",
      "path": ".",
      "scope": "source.leo",
      "file-types": ["leo"],
      "injection-regex": "^leo$",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GPL-3.0",
    "description": "Tree-sitter grammar for the Leo programming language",
    "authors": [
      {
        "name": "Leo Contributors"
      }
    ],
    "links": {
      "repository": "https://github.com/ProvableLabs/leo"
    }
  }
}
